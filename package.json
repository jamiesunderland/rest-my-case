{
  "name": "rest-my-case",
  "version": "1.0.0",
  "description": "An http request helper for converting between cases",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "tsc",
    "test": "./node_modules/.bin/jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:coverage": "node ./node_modules/jest/bin/jest.js --coverage",
    "test:coverage:reporter": "node ./node_modules/jest/bin/jest.js --coverage --coverageDirectory ./coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/isomorphic-fetch": "^0.0.34",
    "change-case": "^3.0.2",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "save-dev": "^2.0.0",
    "ts-node": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamiesunderland/rest-my-case.git"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "^22.1.2",
    "babel-jest": "^22.2.2",
    "jest": "^22.2.2",
    "json2typescript": "^1.0.5",
    "mockttp": "^0.9.0",
    "ts-jest": "^22.0.3",
    "typescript": "^2.7.1",
    "typings": "^2.1.1",
    "yarn": "^1.3.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/test/*.spec.(ts|tsx|js)"
    ],
    "testURL": "http://localhost/"
  }
}
